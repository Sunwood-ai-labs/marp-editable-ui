{
  "name": "marp-editable-slides-sample",
  "version": "1.0.0",
  "description": "Marpの編集機能を試せるミニマルなサンプルリポジトリ",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "marp",
    "slides",
    "presentation",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@marp-team/marp-cli": "^4.1.0",
    "@marp-team/marpit": "^3.1.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.9"
  }
}
